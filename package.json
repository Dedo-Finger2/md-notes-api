        "@eslint/js": "^9.8.0",
        "eslint": "^9.8.0",
        "globals": "^15.8.0",
        "husky": "^9.1.3",
        "lint-staged": "^15.2.7",
        "prettier": "^3.3.3",
        "supertest": "^7.0.0",
        "vitest": "^2.0.4"
    },
    "dependencies": {
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "octokit": "^4.0.2",
        "valibot": "^0.36.0"
